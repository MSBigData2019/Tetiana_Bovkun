     "execution_count": 52,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "%%sql\n",
    "DROP VIEW IF EXISTS buveurs_achats;\n",
    "\n",
    "CREATE VIEW buveurs_achats AS\n",
    "SELECT DISTINCT NB, NOM, PRENOM, QTE\n",
    "FROM BUYER \n",
    "INNER JOIN SALES USING (NB)\n",
    "WHERE NB NOT IN (\n",
    "    SELECT DISTINCT NB\n",
    "    from BUYER INNER JOIN SALES USING (NB) \n",
    "    WHERE QTE is null)"
   ]
  },
