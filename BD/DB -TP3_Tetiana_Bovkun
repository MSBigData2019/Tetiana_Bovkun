    "%%sql DROP VIEW IF EXISTS buveurs_asec;\n",
    "CREATE VIEW buveurs_asec AS\n",
    "SELECT DISTINCT *\n",
    "FROM BUYER\n",
    "INNER JOIN SALES USING (NB)\n",
    "WHERE QTE = 0"
   ]
