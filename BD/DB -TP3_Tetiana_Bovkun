    "%%sql UPDATE BUYER \n",
    "SET TYPE = 'gros'\n",
    "WHERE BUYER.NB IN \n",
    "(SELECT NB FROM BUYER\n",
    "INNER JOIN SALES USING (NB) GROUP BY NB HAVING SUM(QTE) > 100 AND TYPE <> 'gros')"
   ]
  },
