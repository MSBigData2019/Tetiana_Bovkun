   "source": [
    "%%sql\n",
    "\n",
    "DROP TABLE IF EXISTS RBA;\n",
    "CREATE TABLE RBA(NB INT, NOM VARCHAR, PRENOM VARCHAR, TYPE VARCHAR);\n",
    "INSERT INTO RBA(NB, NOM, PRENOM, TYPE)\n",
    "SELECT NB, NOM, PRENOM, TYPE\n",
    "FROM buveurs_asec;"
   ]
  },
