     },
     "execution_count": 57,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "%%sql\n",
    "\n",
    "DROP TABLE IF EXISTS RBB;\n",
    "CREATE TABLE RBB(NB INT, NOM VARCHAR, PRENOM VARCHAR);\n",
    "INSERT INTO RBB(NB, NOM, PRENOM)\n",
    "SELECT NB, NOM, PRENOM\n",
    "FROM bons_buveurs;"
   ]
  },
